    "from iminuit import Minuit\n",
    "def f(x, y, z):\n",
    "    return (x - 2) ** 2 + (y - 3) ** 2 + (z - 4) ** 2\n",
    "m = Minuit(f, limit_x=(1, 1.4))\n",
    "re = m.migrad()  # run optimiser"
   ]
